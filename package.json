{
    "name": "date-to-format",
    "displayName": "Date To Format",
    "description": "Simple VS Code extension to quickly get the date-format of a given date.",
    "icon": "https://github.com/JonaNeu/date-to-format/blob/master/media/icon.png",
    "version": "1.0.0",
    "publisher": "JonaNeumeier",
    "keywords": [
        "date to format",
        "date",
        "dateformat",
        "date to dateformat"
    ],
    "author": {
        "name": "Jona Neumeier"
    },
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.date-to-format"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.date-to-format",
                "title": "Date to Format"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "eslint": "^5.13.0",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "moment": "^2.24.0"
    }
}
